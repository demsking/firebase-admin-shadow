  "license": "MIT",
  "bugs": {
    "url": "https://github.com/demsking/firebase-admin-shadow/issues"
  },
  "homepage": "https://github.com/demsking/firebase-admin-shadow#readme",
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "standard": "^10.0.3"
  }
}
